age = 18

#if-elif-else chain
if age < 2:
    #print the message
    print("You're a baby!")
elif age < 4:
    #print the message
    print("You're a toddler!")
elif age < 13:
    #print the message
    print("You're a kid!")
elif age < 20:
    #print the message
    print("You're a teenager!")
elif age < 65:
    #print the message
    print("You're an adult!")
else:
    #print the message
    print("You're an elder!")
